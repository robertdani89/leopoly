{
  "name": "backend-word-chain",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/backend/word-chain/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/backend/word-chain",
        "tsConfig": "libs/backend/word-chain/tsconfig.lib.json",
        "packageJson": "libs/backend/word-chain/package.json",
        "main": "libs/backend/word-chain/src/index.ts",
        "assets": ["libs/backend/word-chain/*.md"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/backend/word-chain/jest.config.ts"
      }
    }
  },
  "tags": []
}
